<% content_for(:title, 'Wikipages: All Contacts') %>

<h1>Contacts</h1>

<p>
  <%= link_to('Create new contact', new_contact_path, class: 'btn btn-primary') %>
</p>

<ul>
  <% @contacts.each do |contact| %>
    <li>
      <%= link_to(contact.name, contact) %>
    </li>
  <% end %>
</ul>
